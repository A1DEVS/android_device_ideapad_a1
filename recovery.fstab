/data           ext3    /dev/block/mmcblk0p3
/.secure        ext3    /dev/block/mmcblk0p4
/emmc           vfat    /dev/block/mmcblk0p5
/sdcard         vfat    /dev/block/mmcblk1p1    /dev/block/mmcblk1
/sd-ext         ext3    /dev/block/mmcblk1p2
